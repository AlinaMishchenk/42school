/*compares the first n bytes of memory area str1 and memory area str2*//*#include ¨libft.h¨*/
#include <stdio.h>
int     ft_memcmp(const void *str1, const void *str2, size_t n)
{
    size_t i;
    const unsigned char *p1;/*sravnivaem konkretnie bytes*/
    const unsigned char *p2;

    i = 0;
    p1 = (const unsigned char *)str1;
    p2 = (const unsigned char *)str2;
}
int main ()
{
    char str1[] = "Hello";
    char str2[] = "Bye";

    printf ("%d\n", ft_memcmp(str1, str2, 3));/*sravnivaem stroki*/
        return(0);
}
